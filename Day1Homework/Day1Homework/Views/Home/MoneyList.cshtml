@using Day1Homework.Models.ViewModels;
@model List<MoneyRecViewModel>

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.MoneyType</td>
                    <td>@item.CreDate</td>
                    <td>@item.Amount</td>
                </tr>
            }
        </table>
    </div>
</div>